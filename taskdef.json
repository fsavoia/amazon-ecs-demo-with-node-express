{
  "containerDefinitions": [
      {
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/task-poc-app",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs"
          }
        },
        "portMappings": [
          {
            "hostPort": 3000,
            "protocol": "tcp",
            "containerPort": 3000
          }
        ],
        "cpu": 0,
        "memoryReservation": 256,
        "image": "652839185683.dkr.ecr.us-east-1.amazonaws.com/sample-app:<REVISION>",
        "name": "poc-app"
      }
    ],
    "cpu": "512",
    "executionRoleArn": "arn:aws:iam::652839185683:role/ecsTaskExecutionRole",
    "family": "task-poc-app",
    "memory": "1024",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE" 
     ]
  }