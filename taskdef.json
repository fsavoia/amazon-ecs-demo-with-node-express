{
    "containerDefinitions": [
      {
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/task-poc-app",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs"
          }
        },
        "portMappings": [
          {
            "hostPort": 3000,
            "protocol": "tcp",
            "containerPort": 3000
          }
        ],
        "cpu": 0,
        "memoryReservation": 256,
        "image": "349396007468.dkr.ecr.us-east-1.amazonaws.com/sample-app:<REVISION>",
        "name": "poc-app"
      }
    ]
  }